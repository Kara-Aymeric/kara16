<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="commission_details_wizard_form_view" model="ir.ui.view">
        <field name="name">commission_details_wizard_form_view</field>
        <field name="model">commission.details.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="commission_calcul_ids" nolabel="1" readonly="1">
                            <tree>
                                <field name="order_id" />
                                <field name="partner_id" />
                                <field name="has_follower_technician" />
                                <field name="has_discount_condition" />
                                <field name="order_amount_untaxed" sum="Total untaxed"/>
                                <field name="amount" sum="Total result" />
                            </tree>
                        </field>
                    </group>
                    <footer>
                        <button string="Cancel"
                                class="btn-secondary"
                                special="cancel"
                        />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>