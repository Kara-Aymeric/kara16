#. module: custom_sale
#: model:mail.template,body_html:custom_sale.email_template_supplier_sale
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        <t t-set=\"doc_name\" t-value=\"'quotation' if object.state in ('draft', 'sent') else 'order'\"/>\n"
"        Hello Mr or Mrs <span style=\"font-weight: bold;\" t-out=\"object.supplier_id.name or ''\"/>,\n"
"        <br/><br/>\n"
"        Attached you will find a new order n°<span style=\"font-weight: bold;\" t-out=\"object.name or ''\"/>\n"
"        of <span style=\"font-weight: bold;\" t-out=\"object.customer_id.name or ''\"/>.\n"
"        <br/><br/>\n"
"        Could you prepare the confirmation and send it to me for customer signature please ?\n"
"        <br/><br/>\n"
"        Thanks,\n"
"        <br/><br/>\n"
"        Best regards,\n"
"        <t t-if=\"not is_html_empty(object.user_id.signature)\">\n"
"            <br/><br/>\n"
"            <t t-out=\"object.user_id.signature or ''\">--<br/>Mitchell Admin</t>\n"
"        </t>\n"
"        <br/><br/>\n"
"    </p>\n"
"</div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        <t t-set=\"doc_name\" t-value=\"'devis' if object.state in ('draft', 'sent') else 'commande'\"/>\n"
"        Bonjour M. ou Mme <span style=\"font-weight: bold;\" t-out=\"object.supplier_id.name or ''\"/>,\n"
"        <br/><br/>\n"
"        Vous trouverez ci-joint une nouvelle commande n°<span style=\"font-weight: bold;\" t-out=\"object.name or ''\"/>\n"
"        de <span style=\"font-weight: bold;\" t-out=\"object.customer_id.name or ''\"/>.\n"
"        <br/><br/>\n"
"        Pourriez-vous préparer la confirmation et me l'envoyer pour signature client s'il vous plaît ?\n"
"        <br/><br/>\n"
"        Merci,\n"
"        <br/><br/>\n"
"        Bien cordialement,\n"
"        <t t-if=\"not is_html_empty(object.user_id.signature)\">\n"
"            <br/><br/>\n"
"            <t t-out=\"object.user_id.signature or ''\">--<br/>Mitchell Admin</t>\n"
"        </t>\n"
"        <br/><br/>\n"
"    </p>\n"
"</div>\n"
"            "

#. module: custom_sale
#: model:mail.template,body_html:custom_sale.email_template_supplier_quote_signed
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        <t t-set=\"doc_name\" t-value=\"'quotation' if object.state in ('draft', 'sent') else 'order'\"/>\n"
"        Hello,\n"
"        <br/><br/>\n"
"        Attached you will find Confirmation n°<span style=\"font-weight: bold;\" t-out=\"object.name or ''\"/>\n"
"        signed for the client <span style=\"font-weight: bold;\" t-out=\"object.customer_id.name or ''\"/>.\n"
"        <br/><br/>\n"
"        Good reception,\n"
"        <br/><br/>\n"
"        Best regards,\n"
"        <t t-if=\"not is_html_empty(object.user_id.signature)\">\n"
"            <br/><br/>\n"
"            <t t-out=\"object.user_id.signature or ''\">--<br/>Mitchell Admin</t>\n"
"        </t>\n"
"        <br/><br/>\n"
"    </p>\n"
"</div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        <t t-set=\"doc_name\" t-value=\"'devis' if object.state in ('draft', 'sent') else 'commande'\"></t>\n"
"        Bonjour,\n"
"        <br><br>\n"
"        Vous trouverez ci-joint la confirmation n°<span style=\"font-weight: bold;\" t-out=\"object.name or ''\"></span>\n"
"        signé pour le client <span style=\"font-weight: bold;\" t-out=\"object.customer_id.name or ''\"></span>.\n"
"        <br><br>\n"
"        Bonne réception,\n"
"        <br><br>\n"
"        Bien cordialement,\n"
"        <t t-if=\"not is_html_empty(object.user_id.signature)\">\n"
"            <br><br>\n"
"            <t t-out=\"object.user_id.signature or ''\">--<br>Mitchell Admin</t>\n"
"        </t>\n"
"        <br><br>\n"
"    </p>\n"
"</div>\n"
"            "

